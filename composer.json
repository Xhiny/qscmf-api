{
    "name": "quansitech/qscmf-api",
    "description": "qscmf library api rest",
    "type": "library",
    "keywords": ["qscmf", "api", "rest"],
    "license": "MIT",
    "authors": [
        {
            "name": "tiderjian",
            "email": "tider_yatsen@hotmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "tiderjian/think-core": ">=12.14.0",
        "zircote/swagger-php": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "QscmfApiCommon\\": "src/Common/",
            "QscmfApi\\": "src/Api/",
            "QscmfCrossApi\\": "src/CrossApi/"
        }
    },
    "extra": {
        "qscmf": {
            "providers": [
                "QscmfApiCommon\\QscmfApiCommonProvider",
                "QscmfApi\\QscmfApiProvider",
                "QscmfCrossApi\\QscmfCrossApiProvider"
            ]
        }
    }
}
